  mass1 <- sample(seq(from=200,to=1000,by=1),1)
  
  time1 <- sample(seq(from=20,to=100,by=1),1)
  
  temp1 <- sample(seq(from=150,to=300,by=1),1)
  
  resp1 <- round((mass1*2*10^-2*temp1)*4.186/(time1), digits = 1)
  
