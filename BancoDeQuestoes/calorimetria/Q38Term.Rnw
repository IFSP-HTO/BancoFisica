  temp1  <- sample(seq(from=273,to=303,by=1),1)
  
  temp2  <- sample(seq(from=310,to=330,by=1),1)
  
  vol1   <- sample(seq(from=2,to=10,by=1),1)
  
  resp1  <- round((vol1*temp2)/temp1, digits = 1)
  
